{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Excel1",
						"type": "DatasetReference"
					},
					"name": "GasFlowExcelSource"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "RenamingHeaders"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> GasFlowExcelSource",
				"GasFlowExcelSource select(mapColumn(",
				"          each(match(Entry),",
				"               EntryCountry = $$)",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RenamingHeaders"
			]
		}
	}
}